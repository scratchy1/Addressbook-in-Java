package listwitharray;

public class TestListWithArray {

    public static void main(String[] args) {
        // TODO Auto-generated method stub
        ListWithArray2<Person> personList= new ListWithArray2<Person>(3);
        personList.addLast(new Person("Daisy", "Duck"));
        //personList.getSize();
        System.out.println(personList.getLast().getLastName());
        System.out.println(personList.getLast().getFirstName());
        personList.addLast(new Person("Donald", "Duck"));
        System.out.println(personList.getLast().getLastName());
        System.out.println(personList.getLast().getFirstName());
        personList.addLast(new Person("Donald", "Pump"));
        System.out.println(personList.getLast().getLastName());
        System.out.println(personList.getLast().getFirstName());
        personList.removeLast();
        System.out.println(personList.getLast().getLastName());
        System.out.println(personList.getLast().getFirstName());
        System.out.println(personList.getSize());
    }

}
